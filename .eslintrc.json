{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {},
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "rules": {
        "import/extensions": [
          "error",
          "always",
          {
            "ignorePackages": true
          }
        ]
      }
    }
  ]
}